{
    "env": {
        "browser": true
    },

    "rules": {
        "no-console": 2,
        "no-debugger": 2,
        "no-empty": 2,
        "no-obj-calls": 2,
        "no-unreachable": 2,
        "no-dupe-keys": 2,
        "no-func-assign": 2,
        "no-empty-class": 2,
        "no-caller": 2,
        "no-eval": 2,
        "no-with": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-self-compare": 2,
        "no-eq-null": 2,
        "wrap-iife": 2,
        "curly": 2,
        "eqeqeq": 2,
        "use-isnan": 2,
        "camelcase": 2,
        "quotes": [2, "single"],
        "no-loop-func": 2,
        "no-empty-label": 2,
        "no-unused-vars": 2,
        "no-script-url": 2,
        "no-shadow": 2,
        "no-alert": 2,
        "no-wrap-func": 2,
        "semi": 2,

        "no-comma-dangle": 1,
        "no-catch-shadow": 1,
        "no-new": 1,
        "no-multi-str": 1,
        "no-proto": 1,
        "no-iterator": 1,
        "no-else-return": 1,
        "no-global-strict": 1,
        "no-mixed-requires": 1,
        "no-new-array": 1,
        "no-new-object": 1,
        "no-new-wrappers": 1,
        "no-fallthrough": 1,
        "no-octal": 1,
        "new-parens": 1,
        "new-cap": 1,
        "dot-notation": 1,
        "unnecessary-strict": 1,
        "consistent-this": [1, "that"],
        "quote-props": 1,

        "no-use-before-define": 0,
        "no-return-assign": 0,
        "no-underscore-dangle": 0,
        "no-ternary": 0,
        "no-bitwise": 0,
        "brace-style": 0,
        "guard-for-in": 0,
        "complexity": 0,
        "one-var": 0
    }
}
